

{% extends "masterALTE3.jinja2" %}
{% load static %}


{% block inject_css %}

  <link rel="stylesheet" href="{% static 'horizon_pricer/static/plugins/fontawesome-free/css/all.min.css' %}">

  <!-- DataTables -->
  <link rel="stylesheet" href="{% static 'horizon_pricer/static/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css' %}">
  <link rel="stylesheet" href="{% static 'horizon_pricer/static/plugins/datatables-responsive/css/responsive.bootstrap4.min.css' %}">

  <!-- table buttons bootstrap -->
  <link rel="stylesheet" href="{% static 'horizon_pricer/static/plugins/datatables-buttons/css/buttons.bootstrap4.min.css' %}">

  <!-- daterange picker -->
  <link rel="stylesheet" href="{% static 'horizon_pricer/static/plugins/daterangepicker/daterangepicker.css' %}">

{% endblock inject_css %}

  <section class="scripts">
    {% block scripts %}
    {% endblock scripts %}
  </section>

<body>

  <section class="content">

    {% block content %}

      <section class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1>Titre</h1>
            </div>
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item active">Tableau de suivi Taux</li>
              </ol>
            </div>
          </div>
        </div><!-- /.container-fluid -->
      </section>

      <section class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-7">
              <div class="card card-primary">

                <div class="card-header">
                  <h3 class="card-title">Tenors de la courbe secondaire</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                  <div class="form-group">

                    {{ rendered_form_taux | safe }}
                    {{ form | safe }}

                  </div>
                  <table id="example1" class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>date marché</th>
                        <th>Maturité</th>
                        <th>tenors en jours</th>
                        <th>tenors</th>
                      </tr>
                    </thead>
                    <tbody>
                      {% for d in one %}
                      <tr>
                         <td>{{d.date_marche}}</td>
                         <td>{{d.tenors_y}}</td>
                         <td>{{d.tenors_d}}</td>
                         <td>{{d.taux}}</td>

                      </tr>
                      {% endfor %}
                    </tbody>
                    <tfoot>
                    </tfoot>
                  </table>
                </div>
                <!-- /.card-body -->
              </div>
              <!-- /.card -->
            </div>
            <!-- /.col -->
          </div>
          <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
      </section>

    {% endblock %}



    {% block inject_js %}
      <!-- jQuery -->
      <script src="{% static 'horizon_pricer/static/plugins/jquery/jquery.min.js' %}"></script>
      <!-- Bootstrap 4 -->
      <script src="{% static 'horizon_pricer/static/plugins/bootstrap/js/bootstrap.bundle.min.js' %}"></script>
      <!-- DataTables  & Plugins -->
      <script src="{% static 'horizon_pricer/static/plugins/datatables/jquery.dataTables.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-responsive/js/dataTables.responsive.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-responsive/js/responsive.bootstrap4.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-buttons/js/dataTables.buttons.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-buttons/js/buttons.bootstrap4.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/jszip/jszip.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/pdfmake/pdfmake.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/pdfmake/vfs_fonts.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-buttons/js/buttons.html5.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-buttons/js/buttons.print.min.js' %}"></script>
      <script src="{% static 'horizon_pricer/static/plugins/datatables-buttons/js/buttons.colVis.min.js' %}"></script>

      <script src="{% static 'horizon_pricer/static/plugins/daterangepicker/daterangepicker.js' %}"></script>

      <script src="{% static 'horizon_pricer/static/plugins/dropzone/min/dropzone.min.js' %}"></script>



      <script>
        $(function () {
          $("#example1").DataTable({
            "responsive": true, "lengthChange": false, "autoWidth": false,
            "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
          }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
          $('#example2').DataTable({
            "paging": true,
            "lengthChange": false,
            "searching": false,
            "ordering": true,
            "info": true,
            "autoWidth": false,
            "responsive": true,
          });
        });
      </script>
    {% endblock inject_js %}

  </section>

</body>
